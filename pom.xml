	<parent>
		<groupId>com.lapsa.maven.pom</groupId>
		<artifactId>javaee7-app-parent</artifactId>
		<version>2.0</version>
	</parent>

	<groupId>kz.theeurasia.eurasia36.crm</groupId>
	<artifactId>parent</artifactId>
	<version>1.8-SNAPSHOT</version>
	<packaging>pom</packaging>

	<name>Eurasia36 : CRM : Parent</name>

	<modules>
		<module>webapp</module>
		<module>dependencies</module>
		<module>ear</module>
	</modules>

	<properties>
		<!-- UTILs -->
		<font-awesome-lib.version>4.7.0</font-awesome-lib.version>
		<!-- APIs -->
		<kz-lib.version>2.2</kz-lib.version>
		<international-lib.version>2.0.2</international-lib.version>
		<insurance-lib.version>1.7.3</insurance-lib.version>
		<lapsa-lib.version>1.2</lapsa-lib.version>
		<donkeyfaces-lib.version>1.1</donkeyfaces-lib.version>
		<reports-lib.version>1.0</reports-lib.version>
		<!-- EJBs & WARs -->
		<insurance-services-impl-cdi.version>1.4.1</insurance-services-impl-cdi.version>
		<insurance-dao-impl-jpa.version>1.7-SNAPSHOT</insurance-dao-impl-jpa.version>
		<!-- external -->
		<primefaces.version>5.3.16</primefaces.version>
		<primefaces-themes.version>1.0.10</primefaces-themes.version>
		<primefaces-extensions.version>4.0.0</primefaces-extensions.version>
		<omnifaces.version>2.5.1</omnifaces.version>
	</properties>

