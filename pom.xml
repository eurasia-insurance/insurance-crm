<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.lapsa.maven.pom</groupId>
		<artifactId>javaee7-app-parent</artifactId>
		<version>1.3.1</version>
	</parent>

	<groupId>kz.theeurasia.eurasia36.crm</groupId>
	<artifactId>parent</artifactId>
	<version>1.0-rc1</version>
	<packaging>pom</packaging>

	<name>Eurasia36 : CRM : Parent</name>

	<modules>
		<module>webapp</module>
		<module>ear</module>
	</modules>

	<properties>
		<kz-lib.version>2.0</kz-lib.version>
		<international-lib.version>2.0</international-lib.version>
		<insurance-lib.version>1.3</insurance-lib.version>
		<insurance-persistence-impl-jpa.version>1.0</insurance-persistence-impl-jpa.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<!-- own -->
			<dependency>
				<groupId>kz.theeurasia.eurasia36.crm</groupId>
				<artifactId>eurasia36-crm-webapp</artifactId>
				<version>${project.version}</version>
				<type>war</type>
			</dependency>
			<dependency>
				<groupId>kz.theeurasia.eurasia36.crm</groupId>
				<artifactId>eurasia36-crm-ear</artifactId>
				<version>${project.version}</version>
				<type>ear</type>
			</dependency>
			<!-- internal -->
			<dependency>
				<groupId>com.lapsa.kz-lib</groupId>
				<artifactId>kz-core</artifactId>
				<version>${kz-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.kz-lib</groupId>
				<artifactId>kz-eelib</artifactId>
				<version>${kz-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.international-lib</groupId>
				<artifactId>international-core</artifactId>
				<version>${international-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.international-lib</groupId>
				<artifactId>international-eelib</artifactId>
				<version>${international-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.insurance-lib</groupId>
				<artifactId>insurance-elements</artifactId>
				<version>${insurance-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.insurance-lib</groupId>
				<artifactId>insurance-domain</artifactId>
				<version>${insurance-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.insurance-lib</groupId>
				<artifactId>insurance-validation</artifactId>
				<version>${insurance-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.insurance-lib</groupId>
				<artifactId>insurance-eelib</artifactId>
				<version>${insurance-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.insurance-lib</groupId>
				<artifactId>insurance-persistence</artifactId>
				<version>${insurance-lib.version}</version>
			</dependency>
			<dependency>
				<groupId>com.lapsa.insurance-lib</groupId>
				<artifactId>insurance-persistence-impl-jpa</artifactId>
				<version>${insurance-persistence-impl-jpa.version}</version>
				<type>ejb</type>
			</dependency>
			<!-- external -->
			<dependency>
				<groupId>org.primefaces</groupId>
				<artifactId>primefaces</artifactId>
				<version>5.3</version>
			</dependency>
			<dependency>
				<groupId>org.primefaces.extensions</groupId>
				<artifactId>primefaces-extensions</artifactId>
				<version>4.0.0</version>
			</dependency>
			<dependency>
				<groupId>org.omnifaces</groupId>
				<artifactId>omnifaces</artifactId>
				<version>2.3</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

	<organization>
		<name>Eurasia Insurance Company</name>
		<url>http://www.theeurasia.kz</url>
	</organization>

	<scm>
		<connection>scm:git:https://gitrepo.theeurasia.kz/eurasia36/eurasia36-crm.git</connection>
		<developerConnection>scm:git:https://gitrepo.theeurasia.kz/eurasia36/eurasia36-crm.git</developerConnection>
		<tag>v1.0-rc1</tag>
	</scm>

	<issueManagement>
		<system>gitlab</system>
		<url>https://gitrepo.theeurasia.kz/eurasia36/eurasia36-crm/issues</url>
	</issueManagement>

</project>
